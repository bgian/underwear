{
    "name": "Underwear",
    "version": "1.0.0",
    "description": "Underwear is a simple and lightweight node framework for most applications.",
    "main": "index.js",
    "scripts": {
        "test-unit": "istanbul cover _mocha $(find ./test -name \"*.spec.js\" -not -path \"./node_modules/*\")",
        "new-route": "node scripts/route.js generate",
        "start": "node index.js",
        "start-watch": "nodemon index.js",
        "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "hot": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "production": "cross-env NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "author": "Joseph Smith",
    "license": "ISC",
    "devDependencies": {
        "babel-loader": "^7.1.1",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "browser-sync": "^2.0.0",
        "cross-env": "^4.0.0",
        "istanbul": "^0.4.5",
        "laravel-mix": "^1.4.2",
        "mocha": "^3.5.0",
        "request": "^2.81.0",
        "sass-loader": "^6.0.6",
        "sinon": "^2.4.1",
        "sinon-chai": "^2.12.0",
        "webpack": "^3.4.1"
    },
    "dependencies": {
        "axios": "^0.16.1",
        "bcrypt-nodejs": "0.0.3",
        "body-parser": "^1.17.2",
        "chai": "^4.1.0",
        "connect-flash": "^0.1.1",
        "connect-mongo": "^1.3.2",
        "connect-redis": "^3.3.0",
        "consolidate": "^0.14.5",
        "cookie-parser": "^1.4.3",
        "dotenv": "^4.0.0",
        "ejs": "^2.5.7",
        "express": "^4.15.3",
        "express-session": "^1.15.4",
        "fs": "0.0.1-security",
        "jsonwebtoken": "^7.4.1",
        "lodash": "^4.17.4",
        "make-runnable": "^1.3.6",
        "mongoose": "^4.10.8",
        "passport": "^0.3.2",
        "passport-jwt": "^2.2.1",
        "passport-local": "^1.0.0",
        "validator": "^8.0.0",
        "vue": "^2.2.6",
        "vue-loader": "^13.0.2",
        "vue-router": "^2.7.0",
        "vue-style-loader": "^3.0.1",
        "vue-template-compiler": "^2.4.2",
        "vuex": "^2.3.1",
        "bootstrap-sass": "^3.3.7"
    }
}
